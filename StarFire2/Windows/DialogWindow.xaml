<Window x:Class="StarFire2.Windows.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StarFire2.Windows"
        mc:Ignorable="d"
        Title="DialogWindow" Height="250" Width="450">
    <Grid>
        <Label Content="Заказ готов?" FontSize="24" HorizontalAlignment="Left" Margin="140,35,0,0" VerticalAlignment="Top"/>
        <Button IsCancel="True" Style="{DynamicResource StyleBtn}" Content="Нет" Background="#FFCD07" Foreground="White" FontSize="18" Width="90" Height="50" HorizontalAlignment="Left" Margin="74,122,0,0" VerticalAlignment="Top"/>
        <Button IsDefault="True" Style="{DynamicResource StyleBtn}" Content="Да" Background="#FF4F61" Height="50" FontSize="18" Foreground="White" HorizontalAlignment="Left" Margin="246,122,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click"/>

    </Grid>
</Window>
